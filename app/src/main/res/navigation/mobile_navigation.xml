<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.foodapp.ui.karte.HomeFragment"
        android:label="Karte"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.foodapp.ui.liste.DashboardFragment"
        android:label="Liste"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_filter"
        android:name="com.example.foodapp.ui.filter.FilterFragment"
        android:label="Filter"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.foodapp.ui.kommentare.NotificationsFragment"
        android:label="Kommentare"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.foodapp.ui.einstellungen.SettingsFragment"
        android:label="Einstellungen"
        tools:layout="@layout/fragment_home" />


</navigation>